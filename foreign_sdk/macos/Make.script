tell application "Finder"	activateset source_folder to {choose folder with prompt "Please choose the foreign_sdk source folder"} as string	set target_folder to {folder "macos" in folder source_folder}		set f to file "stub_library.mcp" of folder "macos" of folder source_folder		tell application "CodeWarrior IDE 2.1"		activate		open f		Make Project	end tell		activate		if folder "ALS Prolog Foreign SDK" of folder "macos" of folder source_folder exists then		delete folder "ALS Prolog Foreign SDK" of folder "macos" of folder source_folder	end if	set sdk_folder to make folder at target_folder with properties {name:"ALS Prolog Foreign SDK"}	set support_folder to make folder at sdk_folder with properties {name:"ALS Prolog Support"}	duplicate {file "alspi.h" of folder "source" of folder source_folder} to support_folder	duplicate {file "alspi_slib.c" of folder "source" of folder source_folder} to support_folder	duplicate {file "ALSPrologLib" of folder "macos" of folder source_folder} to support_folder		set docs_folder to make folder at sdk_folder with properties {name:"Documentation"}	duplicate {file "FI_Manual.pdf" of folder "docs" of folder source_folder} to docs_folder	set name of file "FI_Manual.pdf" of docs_folder to "Foreign Interface Manual.pdf"	duplicate {file "FI_Ref.pdf" of folder "docs" of folder source_folder} to docs_folder	set name of file "FI_Ref.pdf" of docs_folder to "Foreign Interface Reference.pdf"		set examples_folder to make folder at sdk_folder with properties {name:"Examples"}	duplicate {file "Examples_Read_Me" of folder "macos" of folder "examples" of folder source_folder} to examples_folder	set name of file "Examples_Read_Me" of examples_folder to "Examples Read Me"		set qa_folder to make folder at examples_folder with properties {name:"Q&A Example"}	duplicate {file "Q_and_A_PPC.mcp" of folder "macos" of folder "examples" of folder source_folder} to qa_folder	duplicate {file "qa.c" of folder "common" of folder "examples" of folder source_folder} to qa_folder	duplicate {file "know.pro" of folder "common" of folder "examples" of folder source_folder} to qa_folder		set even_folder to make folder at examples_folder with properties {name:"Even Example"}	duplicate {file "even_PPC.mcp" of folder "macos" of folder "examples" of folder source_folder} to even_folder	duplicate {file "even.c" of folder "common" of folder "examples" of folder source_folder} to even_folder		set tutorial_folder to make folder at examples_folder with properties {name:"Tutorial Examples"}	duplicate {file "examples_PPC.mcp" of folder "macos" of folder "examples" of folder source_folder} to tutorial_folder	duplicate {file "examples.c" of folder "common" of folder "examples" of folder source_folder} to tutorial_folder	end tell