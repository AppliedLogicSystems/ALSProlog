#
# Makefile.in is the Makefile template for alspro
# Makefile is generated from Makefile.in by running configure
#
# If you edit Makefile, your changes will be lost when you run configure
# again.
#

srcdir = @srcdir@

BUILDS	= bld-port bld-natv

.PHONY: all

all: 
	(cd bld-port; $(MAKE) all)
	-(cd bld-natv; $(MAKE) all)
	echo Using $(srcdir) to make bld-wins
	./bld-port/alspro $(srcdir)/pconfig/pconfig -g pconfig
	cd ./bld-wins; $(MAKE) all

testrun:
	cd ./bld-port; $(MAKE) testrun
	cd ./bld-natv; $(MAKE) testrun

cleanup:
	cd ./bld-port; $(MAKE) cleanup
	cd ./bld-natv; $(MAKE) cleanup
	cd ./bld-wins; $(MAKE) cleanup

super_clean:
	cd ./bld-port; $(MAKE) super_clean
	cd ./bld-natv; $(MAKE) super_clean
	cd ./bld-wins; $(MAKE) super_clean
