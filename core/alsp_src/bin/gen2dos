/mailbox3/alsp_src/bin/u2d $1/bld-port/lexinit.h
cp /mailbox3/bldtests/hp5/bld-port/siolex.h $1/bld-port
/mailbox3/alsp_src/bin/u2d $1/bld-port/siolex.h
cp /mailbox3/bldtests/hp5/bld-port/tokens.h $1/bld-port
/mailbox3/alsp_src/bin/u2d $1/bld-port/tokens.h
cp /mailbox3/bldtests/hp5/bld-port/tokini2.h $1/bld-port
/mailbox3/alsp_src/bin/u2d $1/bld-port/tokini2.h
cp /mailbox3/alsp_src/generic/pi_cfg.in $1/bld-port/pi_cfg.h
/mailbox3/alsp_src/bin/u2d $1/bld-port/pi_cfg.h

