Set Echo 1Echo Setup {2}If !"`Exists -d {2}`"	NewFolder {2}EndEcho Setup top levelDuplicate -y {1}cfgs:maketop.mac {2}MakefileDuplicate -y {1}port:macos:macos_config.h {2}config.hEcho Setup bld-portIf !"`Exists -d {2}bld-port:`"	NewFolder {2}bld-port:EndDuplicate -y {1}bld-port:makefile.mac {2}bld-port:MakefileStreamEdit {2}bld-port:Makefile -set srcdir={1} -e '/@srcdir@/ Replace // srcdir -c °' -o {2}bld-port:MakefileDuplicate -y {1}bld-port:tconfig.h.in {2}bld-port:tconfig.hDuplicate -y {1}generic:pi_cfg.in {2}bld-port:pi_cfg.hIf !"`Exists -d {2}bld-port:alsdir:`"	NewFolder {2}bld-port:alsdir:EndIf !"`Exists -d {2}bld-port:alsdir:builtins:`"	NewFolder {2}bld-port:alsdir:builtins:EndIf !"`Exists -d {2}bld-port:alsdir:library:`"	NewFolder {2}bld-port:alsdir:library:EndDuplicate -y {1}builtins:Å.pro {2}bld-port:alsdir:builtins:Duplicate -y {1}library:Å.pro {2}bld-port:alsdir:library:Duplicate -y {1}library:Å.alb {2}bld-port:alsdir:library: