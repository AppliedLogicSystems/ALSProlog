set BUILDPATH to "xxx:builds:i386_mswin32:"set PORTPATH to BUILDPATH & "bld-port:"set WINPATH to BUILDPATH & "bld-wins:"with timeout of 1000 seconds		tell application "CodeWarrior IDE 1.6.2"		activate		open file {PORTPATH & "alspro_lib.prj"}		Make Project		Close Project		open file {PORTPATH & "alspro_dll.prj"}		Make Project		Close Project		open file {PORTPATH & "alspro_b.prj"}		Make Project		Close Project		open file {PORTPATH & "demo_alspro_lib.prj"}		Make Project		Close Project		open file {PORTPATH & "demo_alspro_dll.prj"}		Make Project		Close Project		open file {PORTPATH & "demo_alspro_b.prj"}		Make Project		Close Project		open file {PORTPATH & "testdll.x86.prj"}		Make Project		Close Project		open file {PORTPATH & "demo_testdll.x86.prj"}		Make Project		Close Project	end tell	end timeout