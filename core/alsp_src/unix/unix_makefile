# Generic Unix Definitions for GNU Make file for ALS Prolog

# Common defaults for most unix systems. May be overiden by $(SUBOS)_makefile
OS_AR = ar rusc
OS_RANLIB = true
OS_SL_EXT = so
OS_SL_LINK = $(CC) -shared
LIBS = $(OS_LIBS) -lc -lm
SL_LINK_LIBS = $(LIBS)

include $(SOURCE_DIR)/unix/$(SUBOS)_makefile

OS_CPPFLAGS=-DUNIX $(SUBOS_CPPFLAGS)

OS_SOURCE = unixio.c missing.c

