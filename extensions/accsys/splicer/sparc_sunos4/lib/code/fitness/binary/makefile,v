@#  
#  Makefile for splicer library: code/fitness/binary
# 

VPATH=/mailbox3/splicer/code/fitness/binary


LclIncs= -I/mailbox3/splicer/include 
LclFlags = 

BASELIB=binary.a



all: library 

CC = gcc
LINK = $(CC)
CFLAGS = -O -g -Wall -Wshadow -Wconversion $(PROTOFLAGS)
LIBS = -lnsl -lm
#
#
CPPFLAGS = -I. $(LclIncs) $(XINCLUDES) $(LclFlags)
.c.o:
	$(CC) -c $(CPPFLAGS) $(CFLAGS) $(X_CFLAGS) $<

BASEOBJS	= fitnessMaxPolygon.o \
fitnessMinPolygon.o \
fitnessSumSquares.o 

library: $(BASELIB)


$(BASELIB): $(BASEOBJS)
	echo BASEOBJS=$(BASEOBJS)
	ar ruv $(BASELIB) $(BASEOBJS)
	ranlib $(BASELIB)


# Dependencies:

fitnessMaxPolygon.o : fitnessMaxPolygon.c 
fitnessMinPolygon.o : fitnessMinPolygon.c 
fitnessSumSquares.o : fitnessSumSquares.c 
