@#  
#  Makefile for splicer library: code
# 

VPATH=/mailbox3/splicer/code


LclIncs= -I/mailbox3/splicer/include 
LclFlags = 

BASELIB=code.a



all: library fitness ga uif 

CC = gcc
LINK = $(CC)
CFLAGS = -O -g -Wall -Wshadow -Wconversion $(PROTOFLAGS)
LIBS = -lnsl -lm
#
#
CPPFLAGS = -I. $(LclIncs) $(XINCLUDES) $(LclFlags)
.c.o:
	$(CC) -c $(CPPFLAGS) $(CFLAGS) $(X_CFLAGS) $<

BASEOBJS	= 
library: $(BASELIB)


$(BASELIB): $(BASEOBJS)
	echo BASEOBJS=$(BASEOBJS)
	ar ruv $(BASELIB) $(BASEOBJS)
	ranlib $(BASELIB)


# Dependencies:


fitness:
	(cd fitness ; make all)


ga:
	(cd ga ; make all)


uif:
	(cd uif ; make all)

@


