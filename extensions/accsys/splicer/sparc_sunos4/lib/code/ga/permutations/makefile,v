@#  
#  Makefile for splicer library: code/ga/permutations
# 

VPATH=/mailbox3/splicer/code/ga/permutations


LclIncs= -I/mailbox3/splicer/include 
LclFlags = 

BASELIB=libGAp.a



all: library 

CC = gcc
LINK = $(CC)
CFLAGS = -O -g -Wall -Wshadow -Wconversion $(PROTOFLAGS)
LIBS = -lnsl -lm
#
#
CPPFLAGS = -I. $(LclIncs) $(XINCLUDES) $(LclFlags)
.c.o:
	$(CC) -c $(CPPFLAGS) $(CFLAGS) $(X_CFLAGS) $<

BASEOBJS	= gaChromosomesP.o \
gaCrossoverP.o \
gaMutationP.o \
gaParametersP.o \
xParamCharDialog.o 

library: $(BASELIB)


$(BASELIB): $(BASEOBJS)
	echo BASEOBJS=$(BASEOBJS)
	ar ruv $(BASELIB) $(BASEOBJS)
	ranlib $(BASELIB)


# Dependencies:

gaChromosomesP.o : gaChromosomesP.c 
gaCrossoverP.o : gaCrossoverP.c 
gaMutationP.o : gaMutationP.c 
gaParametersP.o : gaParametersP.c 
xParamCharDialog.o : xParamCharDialog.c 
@


