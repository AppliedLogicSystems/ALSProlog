@#  
#  Makefile for splicer library: code/ga
# 

VPATH=/mailbox3/splicer/code/ga


LclIncs= -I/mailbox3/splicer/include 
LclFlags = 

BASELIB=libGA.a



all: library binary permutations 

CC = gcc
LINK = $(CC)
CFLAGS = -O -g -Wall -Wshadow -Wconversion $(PROTOFLAGS)
LIBS = -lnsl -lm
#
#
CPPFLAGS = -I. $(LclIncs) $(XINCLUDES) $(LclFlags)
.c.o:
	$(CC) -c $(CPPFLAGS) $(CFLAGS) $(X_CFLAGS) $<

BASEOBJS	= gaFileio.o \
gaMain.o \
gaMembers.o \
gaPopulation.o \
gaRandom.o \
gaStatistics.o \
gaUtilities.o \
gaSharing.o \
gaSampling.o \
gaSelection.o \
gaScaling.o 

library: $(BASELIB)


$(BASELIB): $(BASEOBJS)
	echo BASEOBJS=$(BASEOBJS)
	ar ruv $(BASELIB) $(BASEOBJS)
	ranlib $(BASELIB)


# Dependencies:

gaFileio.o : gaFileio.c 
gaMain.o : gaMain.c 
gaMembers.o : gaMembers.c 
gaPopulation.o : gaPopulation.c 
gaRandom.o : gaRandom.c 
gaStatistics.o : gaStatistics.c 
gaUtilities.o : gaUtilities.c 
gaSharing.o : gaSharing.c 
gaSampling.o : gaSampling.c 
gaSelection.o : gaSelection.c 
gaScaling.o : gaScaling.c 

binary:
	(cd binary ; make all)


permutations:
	(cd permutations ; make all)

@


