@#  
#  Makefile for splicer library: code/uif
# 

VPATH=/mailbox3/splicer/code/uif


LclIncs= -I/mailbox3/splicer/include 
LclFlags = 

BASELIB=libXI.a



all: library myXlib 

CC = gcc
LINK = $(CC)
CFLAGS = -O -g -Wall -Wshadow -Wconversion $(PROTOFLAGS)
LIBS = -lnsl -lm
#
#
CPPFLAGS = -I. $(LclIncs) $(XINCLUDES) $(LclFlags)
.c.o:
	$(CC) -c $(CPPFLAGS) $(CFLAGS) $(X_CFLAGS) $<

BASEOBJS	= xEvent.o \
xMenus.o \
xWindows.o \
xDialogs.o 

library: $(BASELIB)


$(BASELIB): $(BASEOBJS)
	echo BASEOBJS=$(BASEOBJS)
	ar ruv $(BASELIB) $(BASEOBJS)
	ranlib $(BASELIB)


# Dependencies:

xEvent.o : xEvent.c 
xMenus.o : xMenus.c 
xWindows.o : xWindows.c 
xDialogs.o : xDialogs.c 

myXlib:
	(cd myXlib ; make all)

@


